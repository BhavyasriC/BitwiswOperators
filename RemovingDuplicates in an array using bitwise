package com.sep06;

public class RemovingDuplicate {

	public static void main(String[] args) {
	    int[] arr = new int[] {1,2,3,5,6,8,4,7};
	    int[] arr2 = new int[] {1,3,2,5,4,8,6,7};

	    for(int k=0;k<arr.length&k<arr2.length;k++) {
	    	if(arr[k]==arr2[k]) 
	    		System.out.print(arr[k]+" ");
	    }
	 System.out.println("duplicate elements");

	}

}
